{
	"name": "evoweb/ew-socialfeedwall",
	"type": "typo3-cms-plugin",
	"description": "Displays social feeds like twitter wall",
	"homepage": "http://www.evoweb.de/",
	"license": ["GPL-2.0+"],

	"support": {
		"issues": "https://github.com/evoWeb/ew_socialfeedwall/issues"
	},
	"keywords": [
		"twitter feed"
	],

	"require": {
		"typo3/cms-core": ">=8.7.0"
	},

	"autoload": {
		"psr-4": {
			"Evoweb\\EwSocialfeedwall\\": "Classes/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"TYPO3\\CMS\\Core\\Tests\\": ".Build/Web/typo3/sysext/core/Tests/",
			"Evoweb\\EwSocialfeedwall\\Tests\\": "Tests/"
		}
	},

	"config": {
		"vendor-dir": ".Build/vendor",
		"bin-dir": ".Build/bin",
		"preferred-install": {
			"typo3/cms": "source",
			"*": "dist"
		}
	},
	"extra": {
		"typo3/cms": {
			"cms-package-dir": "{$vendor-dir}/typo3/cms",
			"web-dir": ".Build/Web"
		}
	}
}
