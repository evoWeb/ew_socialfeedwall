plugin.tx_ewsocialfeedwall {
  settings {
    search = {$plugin.tx_ewsocialfeedwall.settings.search}
    limit = {$plugin.tx_ewsocialfeedwall.settings.limit}

    twitter {
      consumer_key = {$plugin.tx_ewsocialfeedwall.settings.twitter.consumer_key}
      consumer_secret = {$plugin.tx_ewsocialfeedwall.settings.twitter.consumer_secret}
      access_token = {$plugin.tx_ewsocialfeedwall.settings.twitter.access_token}
      access_token_secret = {$plugin.tx_ewsocialfeedwall.settings.twitter.access_token_secret}
    }
  }
}

// PAGE object for Ajax call:
ajaxselectlist_page = PAGE
ajaxselectlist_page {
  typeNum = 15072715
  headerData >
  config {
    no_cache = 1
    disableAllHeaderCode = 1
    additionalHeaders.10.header = Content-type: application/json
    additionalHeaders.10.replace = 1
    debug = 0
  }

  10 < tt_content.list.20.ewsocialfeedwall_display
  10.switchableControllerActions.Display.1 = getTweets
}
